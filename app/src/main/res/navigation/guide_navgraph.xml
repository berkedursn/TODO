<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/guide_navgraph"
    app:startDestination="@id/welcomeFirst">

    <fragment
        android:id="@+id/welcomeFirst"
        android:name="com.berkedursunoglu.a.guideactivitys.WelcomeFirst"
        android:label="WelcomeFirst" >
        <action
            android:id="@+id/action_welcomeFirst_to_welcomeSecond"
            app:destination="@id/welcomeSecond"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/welcomeSecond"
        android:name="com.berkedursunoglu.a.guideactivitys.WelcomeSecond"
        android:label="WelcomeSecond" >
        <action
            android:id="@+id/action_welcomeSecond_to_welcomeThirt"
            app:destination="@id/welcomeThirt"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/welcomeThirt"
        android:name="com.berkedursunoglu.a.guideactivitys.WelcomeThirt"
        android:label="WelcomeThirt" />
</navigation>